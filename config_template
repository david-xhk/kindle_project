export production_server=;
export mysql_server=;
export mongodb_server=;
export flask_server=;

export DEV_ADDRESS=;
export DEV_PASSWORD=;
export DEV_KEY_FILE=;
export SSH_OPTIONS="-i $DEV_KEY_FILE -o StrictHostKeyChecking=no -o ConnectionAttempts=20"

export PRODUCTION_HOST=$production_server;
export FLASK_HOST=$flask_server;
export FLASK_SOURCE=flask_server.zip;

export MYSQL_HOST=$mysql_server;
export MYSQL_BIND_ADDRESS=0.0.0.0;
export MYSQL_PORT=3306;
export MYSQL_DB=;
export MYSQL_TABLE=;
export MYSQL_SOURCE=;
export MYSQL_USER=;
export MYSQL_USER_ADDRESS=$FLASK_HOST;
export MYSQL_PASSWORD=;
export MYSQL_ROOT_PASSWORD=;

export MONGO_HOST=$mongodb_server;
export MONGO_BIND_ADDRESS=0.0.0.0;
export MONGO_PORT=27017;
export MONGO_DB=;
export MONGO_COLLECTION=;
export MONGO_SOURCE=;
export MONGO_USER=;
export MONGO_PASSWORD=;
export MONGO_ROOT_PASSWORD=;
